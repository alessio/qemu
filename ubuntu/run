#!/usr/bin/env bash
qemu-system-aarch64 -m 4096 \
 -cpu host -smp cpus=4,sockets=1,cores=4,threads=1 -machine virt -accel hvf \
 -bios ./QEMU_EFI.fd \
 -serial stdio \
 -accel hvf \
 -drive if=virtio,format=raw,file=./ubuntu.img,id=hd0 \
 -device virtio-gpu-pci -device nec-usb-xhci -device usb-tablet -device usb-kbd

